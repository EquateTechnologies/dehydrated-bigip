#!/bin/bash

REDEPLOY_USING_HOOK='hooks/dehydrated-bigip-deploy-management-certificate'

unchanged_cert() {
  ${BASEDIR}/${REDEPLOY_USING_HOOK} deploy_cert ${2} ${3} ${4} ${5} ${6}
}

HANDLER="$1"; shift
if [ -n "$(type -t $HANDLER)" ] && [ "$(type -t $HANDLER)" = function ]; then
  "$HANDLER" "$@"
fi

# EOF
